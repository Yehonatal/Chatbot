{
    "name": "chatbot-backend",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:python": "python3 python_scripts/test_components.py",
        "build": "npm run build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "setup:python": "pip install -r python_scripts/requirements.txt",
        "install:all": "npm install && npm run setup:python",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f",
        "db:seed": "node scripts/seed.js",
        "train:chatbot": "python3 python_scripts/trainer.py",
        "setup:qdrant": "python3 python_scripts/setup_qdrant.py",
        "health:check": "node scripts/health-check.js",
        "clean:logs": "rm -rf logs/*.log"
    },
    "author": "",
    "license": "ISC",
    "description": "Production-ready Node.js/Express backend with RAG chatbot integration",
    "dependencies": {
        "express": "^5.1.0",
        "mongoose": "^8.0.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.2",
        "helmet": "^7.0.0",
        "express-rate-limit": "^7.0.0",
        "express-validator": "^7.0.0",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1",
        "compression": "^1.7.4",
        "joi": "^17.9.2",
        "uuid": "^9.0.1",
        "axios": "^1.12.2"
    },
    "devDependencies": {
        "nodemon": "^3.0.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.57.0",
        "@types/node": "^20.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
